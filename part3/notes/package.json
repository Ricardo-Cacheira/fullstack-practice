  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development node --watch index.js",
    "test": "cross-env  NODE_ENV=test node --test",
    "build:ui": "rm -rf dist && cd ../../part2/notes && npm run build && cp -r dist ../../part3/notes",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push",
    "lint": "eslint .",
    "mongo": "cross-env NODE_ENV=test node mongo.js",
    "only": "npm test -- --test-only",
    "testNote": "npm test -- tests/note_api.test.js",
    "testUser": "npm test -- tests/user_api.test.js",
    "start:test": "cross-env NODE_ENV=test node --watch index.js"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.8.0",
